{
  "name": "eslint-plugin-interface-method-style",
  "description": "ESLint plugin to enforce consistent method style between interfaces and their implementations",
  "version": "1.1.5-beta.0",
  "author": "Egor Avakumov (https://github.com/eavam)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eavam/eslint-plugin-interface-method-style"
  },
  "bugs": {
    "url": "https://github.com/eavam/eslint-plugin-interface-method-style/issues"
  },
  "homepage": "https://github.com/eavam/eslint-plugin-interface-method-style#readme",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "release": "release-it",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "build": "tsc"
  },
  "type": "commonjs",
  "packageManager": "yarn@4.5.2",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.15.0",
    "@release-it/conventional-changelog": "8.0.1",
    "@typescript-eslint/parser": "8.15.0",
    "@typescript-eslint/rule-tester": "8.15.0",
    "@typescript-eslint/utils": "8.15.0",
    "conventional-changelog-cli": "4.1.0",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "globals": "15.12.0",
    "prettier": "3.4.1",
    "release-it": "17.10.0",
    "typescript": "5.7.2",
    "typescript-eslint": "8.16.0",
    "vitest": "2.1.5"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "typescript",
    "interface",
    "method",
    "style",
    "consistency"
  ],
  "release-it": {
    "$schema": "https://unpkg.com/release-it/schema/release-it.json",
    "git": {
      "commitMessage": "chore: release v${version}",
      "tagName": "v${version}",
      "push": true,
      "requireCommits": true,
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "hooks": {
      "before:init": [
        "yarn lint",
        "yarn test --run",
        "yarn build"
      ],
      "after:release": "echo Successfully released ${name} v${version} to npm."
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  }
}
