{
  "name": "eslint-plugin-interface-method-style",
  "description": "ESLint plugin to enforce consistent method style between interfaces and their implementations",
  "version": "1.1.6",
  "author": "Egor Avakumov (https://github.com/eavam)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eavam/eslint-plugin-interface-method-style"
  },
  "bugs": {
    "url": "https://github.com/eavam/eslint-plugin-interface-method-style/issues"
  },
  "homepage": "https://github.com/eavam/eslint-plugin-interface-method-style#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "release": "release-it",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "build": "tsup"
  },
  "type": "module",
  "packageManager": "yarn@4.9.2",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": ">=4.0.0",
    "eslint": ">=8.0.0",
    "typescript": ">=4.0.0"
  },
  "dependencies": {
    "@typescript-eslint/types": "8.34.0",
    "@typescript-eslint/utils": "8.34.0"
  },
  "devDependencies": {
    "@eslint/js": "9.29.0",
    "@release-it/conventional-changelog": "10.0.1",
    "@types/node": "24.0.1",
    "@typescript-eslint/parser": "8.34.0",
    "@typescript-eslint/rule-tester": "8.34.0",
    "@vitest/coverage-v8": "3.2.3",
    "conventional-changelog-cli": "5.0.0",
    "eslint": "9.29.0",
    "eslint-config-prettier": "10.1.5",
    "globals": "16.2.0",
    "prettier": "3.5.3",
    "release-it": "19.0.3",
    "tsup": "8.5.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.34.0",
    "vitest": "3.2.3"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "typescript",
    "interface",
    "method",
    "style",
    "consistency"
  ],
  "release-it": {
    "$schema": "https://unpkg.com/release-it/schema/release-it.json",
    "git": {
      "commitMessage": "chore: release v${version}",
      "tagName": "v${version}",
      "push": true,
      "requireCommits": true,
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "hooks": {
      "before:init": [
        "yarn lint",
        "yarn test --run",
        "yarn build"
      ],
      "after:release": "echo Successfully released ${name} v${version} to npm."
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  }
}
